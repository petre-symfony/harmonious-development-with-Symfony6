{%  extends 'base.html.twig' %}

{% block title %} Create a new Record | {{ parent() }}{% endblock %}
{% block body %}
<h1>{{ title }}</h1>

{# Add an image of the record #}
<div>
	Tracks:

	{{ dump() }}
	<ul>
		{% for track in tracks %}
			<li>
				{{ track.song }} - {{ track.artist|upper|lower }}
			</li>
		{% endfor %}
	</ul>
</div>
{%  endblock %}
